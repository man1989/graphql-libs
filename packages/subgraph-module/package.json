{
  "name": "@graphql-libs/subgraph-module",
  "version": "0.2.3",
  "main": "index.js",
  "author": "Manish Kumar",
  "license": "MIT",
  "private": true,
  "description": "Common library for building domain subgraph",
  "scripts": {
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.3.0",
    "winston": "3.14.2",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "@graphql-tools/schema": "^10.0.6",
    "@graphql-tools/utils": "10.5.4"
  },
  "devDependencies": {
    "@man1989/tsconfig-base": "^0.1.4",
    "@types/node": "22.5.4",
    "dotenv": "16.4.5",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/libs/config/index.d.ts",
      "import": "./dist/libs/config/index.mjs",
      "module": "./dist/libs/config/index.mjs",
      "require": "./dist/libs/config/index.js"
    },
    "./logger": {
      "types": "./dist/libs/logger/index.d.ts",
      "import": "./dist/libs/logger/index.mjs",
      "module": "./dist/libs/logger/index.mjs",
      "require": "./dist/libs/logger/index.js"
    },
    "./datasources": {
      "types": "./dist/libs/datasources/index.d.ts",
      "import": "./dist/libs/datasources/index.mjs",
      "module": "./dist/libs/datasources/index.mjs",
      "require": "./dist/libs/datasources/index.js"
    },
    "./errors": {
      "types": "./dist/libs/errors/index.d.ts",
      "import": "./dist/libs/errors/index.mjs",
      "module": "./dist/libs/errors/index.mjs",
      "require": "./dist/libs/errors/index.js"
    }
  }
}
